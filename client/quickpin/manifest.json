{
  "name": "SnapIt",
  "version": "1.0",
  "manifest_version": 2,
  "description": "SnapIt on any web item",
  "browser_action": {
    "default_icon": "icon_48.png"
  },
   "icons": {
      "128": "icon_128.png",
      "16": "icon_16.png",
      "48": "icon_48.png"
   },  
  "permissions": [ "cookies","<all_urls>","tabs", "contextMenus", "http://*/*", "https://*/*" ],
  "background": { "scripts": ["main.js"] }
}
